kanske_primtal = 2
while kanske_primtal < 100:
    primtal = True
    lägre_tal = 2
    while lägre_tal <= kanske_primtal / 2:
        if kanske_primtal % lägre_tal == 0:
            primtal = False
            break
        lägre_tal += 1
    if primtal:
        print(kanske_primtal)
    kanske_primtal += 1
